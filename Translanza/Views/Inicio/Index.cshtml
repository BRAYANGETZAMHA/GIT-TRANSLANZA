        $(document).ready(function () {
            $('#Contraseña').keypress(function (e) {
                if (e.which == 13) {
                    Logeo();
                }
            })
        })
        function Logeo() {
            //$("#tabla").empty().html('<img style="display:block;margin-top:20px;margin:auto;" src="../Content/img/ui-anim_basic_16x16.gif" />');
            $("#mensaje").empty().append("<img src='Content/img/loading.gif'></img>");
            $.ajax({
                url: "Inicio/InicioSesion",
                type: "post",
                data: { usuario: $("#Usuario").val(), contraseña: $("#Contraseña").val() },
                success: function (data) {
                    if (data.resultado == true) {

                        window.location.href = data.ruta;
                    }
                    else {
                        $("#mensaje").empty().append("<label>Usuario o contraseña incorrecto.</label>");
                    }
                },
                error: function (error) {
                    debugger
                }
            })
        }

